         <xs:sequence>
            <xs:element name="emetteur" type="typeEmetteur" minOccurs="1" maxOccurs="1" />
            <xs:element name="destinataire" type="typeEmetteur" minOccurs="1" maxOccurs="1" />
            <xs:element name="versionmessage" type="xs:string" minOccurs="1" maxOccurs="1" />
            <xs:element name="fluxnum" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1" />
            <xs:element name="datemaj" type="xs:date" minOccurs="1" maxOccurs="1" />
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="structureej">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="nofiness" type="typeNumeroFiness" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="rs" type="typeRaisonSociale" minOccurs="1" maxOccurs="1" nillable="false" />
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="equipmateriellourd">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="nofinessej" type="typeNumeroFiness" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="rsej" type="typeRaisonSociale" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="eml" type="typeCodeEml" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="libeml" type="typeLibelleEml" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="libcourteml" type="typeLibelleCourtEml" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="noautor" type="typeNumeroAutorisation" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="noautorarhgos" type="typeNumArhgos" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="noimplarhgos" type="typeNumArhgos" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="dateautor" type="typeDateAutorisation" minOccurs="1" maxOccurs="1" nillable="true" default="1900-01-01" />
            <xs:element name="nofinesset" type="typeNumeroFiness" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="numserie" type="typeNumSerieEML" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="marque" type="typeMarqueEML" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="rset" type="typeRaisonSociale" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="datelimite" type="typeDateCaducite" minOccurs="1" maxOccurs="1" nillable="true" default="1900-01-01" />
            <xs:element name="indcaduc" type="typeIndicateurCaducite" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="indnatlien" type="typeIndicateurLien" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="datemeo" type="typeDateMeo" minOccurs="1" maxOccurs="1" nillable="true" default="1900-01-01" />
            <xs:element name="datefin" type="typeDateFinMeo" minOccurs="1" maxOccurs="1" nillable="true" default="1900-01-01" />
            <xs:element name="indrempl" type="typeIndicateurRemplacement" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="noautorremplacement" type="typeNumeroAutorisation" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="indsup" type="typeIndicateurSuppression" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="datemaj" type="typeDateMaj" minOccurs="1" maxOccurs="1" nillable="false" default="1900-01-01" />
         </xs:sequence>
      </xs:complexType>
   </xs:element>
</xs:schema>

