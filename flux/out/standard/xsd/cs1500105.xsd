         <xs:enumeration value="cnamts" />
         <xs:enumeration value="CNSA" />
         <xs:enumeration value="cnsa" />
         <xs:enumeration value="ETALAB" />
         <xs:enumeration value="etalab" />
         <xs:enumeration value="FINESS" />
         <xs:enumeration value="finess" />
         <xs:enumeration value="FNMF" />
         <xs:enumeration value="fnmf" />
         <xs:enumeration value="IMOTEP" />
         <xs:enumeration value="imotep" />
         <xs:enumeration value="PHAR" />
         <xs:enumeration value="phar" />
         <xs:enumeration value="RESS" />
         <xs:enumeration value="ress" />
      </xs:restriction>
   </xs:simpleType>
   <xs:element name="fluxfiness">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="horodatage" minOccurs="1" maxOccurs="1" />
            <xs:choice minOccurs="1" maxOccurs="unbounded">
               <xs:element ref="nomenclclienteleavecagr" />
            </xs:choice>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="horodatage">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="emetteur" type="typeEmetteur" minOccurs="1" maxOccurs="1" />
            <xs:element name="destinataire" type="typeEmetteur" minOccurs="1" maxOccurs="1" />
            <xs:element name="versionmessage" type="xs:string" minOccurs="1" maxOccurs="1" />
            <xs:element name="fluxnum" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1" />
            <xs:element name="datemaj" type="xs:date" minOccurs="1" maxOccurs="1" />
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="nomenclclienteleavecagr">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="code" type="typeCC" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="libelle" type="typeLibelleCC" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="libellecourt" type="typeLibelleCourtCC" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="datedeb" type="typeDateCC" minOccurs="1" maxOccurs="1" nillable="false" default="1900-01-01" />
            <xs:element name="datefin" type="typeDateCC" minOccurs="1" maxOccurs="1" nillable="true" default="1900-01-01" />
            <xs:element name="codeagr2" type="typeCodeCCAGR" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="libelleagr2" type="typeLibCCAGR" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="libellecourtagr2" type="typeLibCourtCCAGR" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="codeagr1" type="typeCodeCCAGR" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="libelleagr1" type="typeLibCCAGR" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="libellecourtagr1" type="typeLibCourtCCAGR" minOccurs="1" maxOccurs="1" nillable="true" />
         </xs:sequence>
      </xs:complexType>
   </xs:element>
</xs:schema>

