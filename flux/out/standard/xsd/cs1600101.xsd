<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
   <xs:simpleType name="typeNumeroAutorisation">
      <xs:restriction base="xs:string">
         <xs:pattern value="[0-9]{1,9}" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeNumeroFiness">
      <xs:restriction base="xs:string">
         <xs:pattern value="[0-9][0-9A-Z][0-9]{7}" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeRaisonSociale">
      <xs:restriction base="xs:string">
         <xs:minLength value="1" />
         <xs:maxLength value="38" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeCodeAutorisationArhgos">
      <xs:restriction base="xs:string">
         <xs:minLength value="1" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeActiviteAutre">
      <xs:restriction base="xs:string">
         <xs:minLength value="2" />
         <xs:maxLength value="2" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeLibelleAutreActivite">
      <xs:restriction base="xs:string">
         <xs:minLength value="1" />
         <xs:maxLength value="150" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeModaliteAutreActivite">
      <xs:restriction base="xs:string">
         <xs:minLength value="2" />
         <xs:maxLength value="2" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeLibModaliteAutreActivite">
      <xs:restriction base="xs:string">
         <xs:minLength value="1" />
         <xs:maxLength value="150" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeFormeAutreActivite">
      <xs:restriction base="xs:string">
         <xs:minLength value="2" />
         <xs:maxLength value="2" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeLibelleFormeAutreActivite">
      <xs:restriction base="xs:string">
         <xs:minLength value="1" />
         <xs:maxLength value="150" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeDate">
      <xs:restriction base="xs:date" />
   </xs:simpleType>
   <xs:simpleType name="typeNumDecision">
      <xs:restriction base="xs:string">
         <xs:minLength value="0" />
         <xs:maxLength value="20" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeCodeEvenement">
      <xs:restriction base="xs:string">
         <xs:minLength value="1" />
         <xs:maxLength value="3" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeLibelleEvenement">
      <xs:restriction base="xs:string">
         <xs:minLength value="1" />
         <xs:maxLength value="60" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeResultatVisite">
      <xs:restriction base="xs:string">
         <xs:enumeration value="C" />
         <xs:enumeration value="NC" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeNumOrdre">
      <xs:restriction base="xs:string">
         <xs:pattern value="[0-9]{1,}" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeAACommentaire">
      <xs:restriction base="xs:string">
         <xs:minLength value="0" />
         <xs:maxLength value="750" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeEmetteur">
      <xs:restriction base="xs:string">
         <xs:enumeration value="ANESM" />
         <xs:enumeration value="anesm" />
         <xs:enumeration value="ARHGOS" />
         <xs:enumeration value="arhgos" />
         <xs:enumeration value="ARSIDF" />
         <xs:enumeration value="arsidf" />
         <xs:enumeration value="ASIP" />
         <xs:enumeration value="asip" />
         <xs:enumeration value="BIOMED" />
         <xs:enumeration value="biomed" />
         <xs:enumeration value="CNAMTS" />
         <xs:enumeration value="cnamts" />
         <xs:enumeration value="CNSA" />
         <xs:enumeration value="cnsa" />
         <xs:enumeration value="ETALAB" />
         <xs:enumeration value="etalab" />
         <xs:enumeration value="FINESS" />
         <xs:enumeration value="finess" />
         <xs:enumeration value="FNMF" />
         <xs:enumeration value="fnmf" />
         <xs:enumeration value="IMOTEP" />
         <xs:enumeration value="imotep" />
         <xs:enumeration value="PHAR" />
         <xs:enumeration value="phar" />
         <xs:enumeration value="RESS" />
         <xs:enumeration value="ress" />
      </xs:restriction>
   </xs:simpleType>
   <xs:element name="fluxfiness">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="horodatage" minOccurs="1" maxOccurs="1" />
            <xs:choice minOccurs="1" maxOccurs="unbounded">
               <xs:element ref="autreactivite" />
               <xs:element ref="evenement" />
            </xs:choice>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="horodatage">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="emetteur" type="typeEmetteur" minOccurs="1" maxOccurs="1" />
            <xs:element name="destinataire" type="typeEmetteur" minOccurs="1" maxOccurs="1" />
            <xs:element name="versionmessage" type="xs:string" minOccurs="1" maxOccurs="1" />
            <xs:element name="fluxnum" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1" />
            <xs:element name="datemaj" type="xs:date" minOccurs="1" maxOccurs="1" />
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="autreactivite">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="noautor" type="typeNumeroAutorisation" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="nofinessej" type="typeNumeroFiness" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="rsej" type="typeRaisonSociale" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="nofinesset" type="typeNumeroFiness" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="rset" type="typeRaisonSociale" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="codeautorarhgos" type="typeCodeAutorisationArhgos" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="activite" type="typeActiviteAutre" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="libactivite" type="typeLibelleAutreActivite" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="modalite" type="typeModaliteAutreActivite" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="libmodalite" type="typeLibModaliteAutreActivite" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="forme" type="typeFormeAutreActivite" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="libforme" type="typeLibelleFormeAutreActivite" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="datedecision" type="typeDate" minOccurs="1" maxOccurs="1" nillable="true" default="1900-01-01" />
            <xs:element name="nodecision" type="typeNumDecision" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="etatautorisation" type="typeCodeEvenement" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="libetatautorisation" type="typeLibelleEvenement" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="datevisite" type="typeDate" minOccurs="1" maxOccurs="1" nillable="true" default="1900-01-01" />
            <xs:element name="resultatvisite" type="typeResultatVisite" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="datefin" type="typeDate" minOccurs="1" maxOccurs="1" nillable="true" default="1900-01-01" />
            <xs:element name="datemeo" type="typeDate" minOccurs="1" maxOccurs="1" nillable="true" default="1900-01-01" />
            <xs:element name="datelimitemeo" type="typeDate" minOccurs="1" maxOccurs="1" nillable="true" default="1900-01-01" />
            <xs:element name="datelimitedepot" type="typeDate" minOccurs="1" maxOccurs="1" nillable="true" default="1900-01-01" />
            <xs:element name="datelimvisite" type="typeDate" minOccurs="1" maxOccurs="1" nillable="true" default="1900-01-01" />
            <xs:element name="datemaj" type="typeDate" minOccurs="1" maxOccurs="1" nillable="false" default="1900-01-01" />
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="evenement">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="noautor" type="typeNumeroAutorisation" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="numordre" type="typeNumOrdre" minOccurs="1" maxOccurs="1" nillable="false" default="0" />
            <xs:element name="nature" type="typeCodeEvenement" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="libelle" type="typeLibelleEvenement" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="datedecision" type="typeDate" minOccurs="1" maxOccurs="1" nillable="true" default="1900-01-01" />
            <xs:element name="dateeffet" type="typeDate" minOccurs="1" maxOccurs="1" nillable="true" default="1900-01-01" />
            <xs:element name="numerodecision" type="typeNumDecision" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="commentaire" type="typeAACommentaire" minOccurs="1" maxOccurs="1" nillable="true" />
         </xs:sequence>
      </xs:complexType>
   </xs:element>
</xs:schema>

