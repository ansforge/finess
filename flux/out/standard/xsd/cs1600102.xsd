<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
   <xs:simpleType name="typeNumeroAutorisation">
      <xs:restriction base="xs:string">
         <xs:pattern value="[0-9]{1,9}" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeIdCPOMASR">
      <xs:restriction base="xs:string">
         <xs:minLength value="1" />
         <xs:maxLength value="45" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeNumeroFiness">
      <xs:restriction base="xs:string">
         <xs:pattern value="[0-9][0-9A-Z][0-9]{7}" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeRaisonSociale">
      <xs:restriction base="xs:string">
         <xs:minLength value="1" />
         <xs:maxLength value="38" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeDate">
      <xs:restriction base="xs:date" />
   </xs:simpleType>
   <xs:simpleType name="typeCodeAutorisationArhgos">
      <xs:restriction base="xs:string">
         <xs:minLength value="1" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeIndicateurRegional">
      <xs:restriction base="xs:string">
         <xs:enumeration value="N" />
         <xs:enumeration value="O" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeActiviteSoumiseAReco">
      <xs:restriction base="xs:string">
         <xs:minLength value="2" />
         <xs:maxLength value="2" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeLibelleActiviteSoumiseARec">
      <xs:restriction base="xs:string">
         <xs:minLength value="1" />
         <xs:maxLength value="150" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeModaliteSoumiseAReco">
      <xs:restriction base="xs:string">
         <xs:minLength value="2" />
         <xs:maxLength value="2" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeLibModaliteSoumiseAReco">
      <xs:restriction base="xs:string">
         <xs:minLength value="1" />
         <xs:maxLength value="150" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeFormeSoumiseAReco">
      <xs:restriction base="xs:string">
         <xs:minLength value="2" />
         <xs:maxLength value="2" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeLibelleFormeSoumiseAReco">
      <xs:restriction base="xs:string">
         <xs:minLength value="1" />
         <xs:maxLength value="150" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeCapaciteAutorisee">
      <xs:restriction base="xs:decimal" />
   </xs:simpleType>
   <xs:simpleType name="typeCodeRegion">
      <xs:restriction base="xs:string">
         <xs:pattern value="[0-9]{2}" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeCodeEvenement">
      <xs:restriction base="xs:string">
         <xs:minLength value="1" />
         <xs:maxLength value="3" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeLibelleEvenement">
      <xs:restriction base="xs:string">
         <xs:minLength value="1" />
         <xs:maxLength value="60" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeNumOrdre">
      <xs:restriction base="xs:string">
         <xs:pattern value="[0-9]{1,}" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeASRNumAvenant">
      <xs:restriction base="xs:string">
         <xs:minLength value="1" />
         <xs:maxLength value="45" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="typeCapacite">
      <xs:restriction base="xs:decimal" />
   </xs:simpleType>
   <xs:simpleType name="typeEmetteur">
      <xs:restriction base="xs:string">
         <xs:enumeration value="ANESM" />
         <xs:enumeration value="anesm" />
         <xs:enumeration value="ARHGOS" />
         <xs:enumeration value="arhgos" />
         <xs:enumeration value="ARSIDF" />
         <xs:enumeration value="arsidf" />
         <xs:enumeration value="ASIP" />
         <xs:enumeration value="asip" />
         <xs:enumeration value="BIOMED" />
         <xs:enumeration value="biomed" />
         <xs:enumeration value="CNAMTS" />
         <xs:enumeration value="cnamts" />
         <xs:enumeration value="CNSA" />
         <xs:enumeration value="cnsa" />
         <xs:enumeration value="ETALAB" />
         <xs:enumeration value="etalab" />
         <xs:enumeration value="FINESS" />
         <xs:enumeration value="finess" />
         <xs:enumeration value="FNMF" />
         <xs:enumeration value="fnmf" />
         <xs:enumeration value="IMOTEP" />
         <xs:enumeration value="imotep" />
         <xs:enumeration value="PHAR" />
         <xs:enumeration value="phar" />
         <xs:enumeration value="RESS" />
         <xs:enumeration value="ress" />
      </xs:restriction>
   </xs:simpleType>
   <xs:element name="fluxfiness">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="horodatage" minOccurs="1" maxOccurs="1" />
            <xs:choice minOccurs="1" maxOccurs="unbounded">
               <xs:element ref="activitesoumiseareco" />
               <xs:element ref="avenant" />
            </xs:choice>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="horodatage">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="emetteur" type="typeEmetteur" minOccurs="1" maxOccurs="1" />
            <xs:element name="destinataire" type="typeEmetteur" minOccurs="1" maxOccurs="1" />
            <xs:element name="versionmessage" type="xs:string" minOccurs="1" maxOccurs="1" />
            <xs:element name="fluxnum" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1" />
            <xs:element name="datemaj" type="xs:date" minOccurs="1" maxOccurs="1" />
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="activitesoumiseareco">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="noautor" type="typeNumeroAutorisation" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="idcpom" type="typeIdCPOMASR" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="nofinessej" type="typeNumeroFiness" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="rsej" type="typeRaisonSociale" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="nofinesset" type="typeNumeroFiness" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="rset" type="typeRaisonSociale" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="dateeffetcpom" type="typeDate" minOccurs="1" maxOccurs="1" nillable="true" default="1900-01-01" />
            <xs:element name="datefincpom" type="typeDate" minOccurs="1" maxOccurs="1" nillable="true" default="1900-01-01" />
            <xs:element name="codeautorarhgos" type="typeCodeAutorisationArhgos" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="indicateurregional" type="typeIndicateurRegional" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="activite" type="typeActiviteSoumiseAReco" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="libactivite" type="typeLibelleActiviteSoumiseARec" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="modalite" type="typeModaliteSoumiseAReco" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="libmodalite" type="typeLibModaliteSoumiseAReco" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="forme" type="typeFormeSoumiseAReco" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="libforme" type="typeLibelleFormeSoumiseAReco" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="dateeffetasr" type="typeDate" minOccurs="1" maxOccurs="1" nillable="true" default="1900-01-01" />
            <xs:element name="capaciteautorisee" type="typeCapaciteAutorisee" minOccurs="1" maxOccurs="1" nillable="true" default="0" />
            <xs:element name="regionautorisation" type="typeCodeRegion" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="etatautorisation" type="typeCodeEvenement" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="libetatautorisation" type="typeLibelleEvenement" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="datemaj" type="typeDate" minOccurs="1" maxOccurs="1" nillable="false" default="1900-01-01" />
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="avenant">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="noautor" type="typeNumeroAutorisation" minOccurs="1" maxOccurs="1" nillable="false" />
            <xs:element name="numordre" type="typeNumOrdre" minOccurs="1" maxOccurs="1" nillable="false" default="0" />
            <xs:element name="typeavenant" type="typeCodeEvenement" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="libavenant" type="typeLibelleEvenement" minOccurs="1" maxOccurs="1" nillable="true" />
            <xs:element name="numavenant" type="typeASRNumAvenant" minOccurs="1" maxOccurs="1" nillable="true" default="0" />
            <xs:element name="dateavenant" type="typeDate" minOccurs="1" maxOccurs="1" nillable="true" default="1900-01-01" />
            <xs:element name="anccapacite" type="typeCapacite" minOccurs="1" maxOccurs="1" nillable="true" default="0" />
            <xs:element name="capacite" type="typeCapacite" minOccurs="1" maxOccurs="1" nillable="true" default="0" />
         </xs:sequence>
      </xs:complexType>
   </xs:element>
</xs:schema>

